{
    "name": "romeoz/rock",
    "description": "The Rock PHP framework",
    "keywords": ["framework", "rock"],
    "type": "library",
    "license": "MIT",
    "support": {
        "email": "serggalka@gmail.com",
        "issues": "https://github.com/romeOz/rock/issues",
        "source": "https://github.com/romeOz/rock"
    },
    "authors": [
        {
            "name": "Ruslan",
            "email": "serggalka@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/romeOz/cartographer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "romeoz/rock-components" : "0.10.*",
        "romeoz/rock-di" : "0.10.*",
        "romeoz/rock-sanitize" : "0.10.*",
        "romeoz/rock-validate" : "0.10.*",
        "romeoz/rock-request" : "0.10.*",
        "romeoz/rock-url" : "0.10.*",
        "romeoz/rock-date" : "0.10.*",
        "romeoz/rock-i18n" : "0.10.*",
        "romeoz/rock-file" : "0.10.*",
        "romeoz/rock-access" : "0.10.*",
        "romeoz/rock-template" : "0.10.*",
        "romeoz/rock-widgets" : "0.10.*",
        "romeoz/rock-behaviors" : "0.10.*",
        "romeoz/rock-session" : "0.10.*",
        "romeoz/rock-db" : "0.10.*",
        "filp/whoops": "1.1.*",
        "phpmailer/phpmailer"   : "5.2.*",
        "monolog/monolog": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.4.*",
        "romeoz/rock-image" : "0.10.*",
        "romeoz/rock-cache" : "0.10.*",
        "romeoz/rock-sphinx" : "0.10.*",
        "romeoz/rock-mongodb" : "0.10.*",
        "lusitanian/oauth": "0.3.*",
        "mibe/feedwriter": "1.0.*",
        "tackk/cartographer" :"*@dev",
        "cebe/markdown": "*@dev"
    },
    "suggest": {
        "romeoz/rock-cache" : "required by Rock Cache",
        "romeoz/rock-mongodb" : "required by Rock MongoDB",
        "romeoz/rock-sphinx" : "required by Rock Sphinx",
        "romeoz/rock-image": "required by Rock Image",
        "romeoz/rock-mq": "required by Rock MQ",
        "tackk/cartographer": "required by SitemapResponseFormatter",
        "mibe/feedwriter": "required by RssResponseFormatter",
        "lusitanian/oauth" : "required by rock-authclient extension",
        "videlalvaro/php-amqplib": "required by rock-mq extension"
    },
    "autoload": {
        "psr-4": {
            "rock\\": "framework/core/",
            "rock\\authclient\\" : "framework/extensions/authclient/",
            "apps\\": "apps/"
        },
        "files": ["framework/extensions/phpmorphy/src/common.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}