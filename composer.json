{
    "name": "romeoz/rock",
    "description": "The Rock PHP framework",
    "keywords": ["rock", "framework"],
    "type": "library",
    "license": "MIT",
    "support": {
        "email": "serggalka@gmail.com",
        "issues": "https://github.com/romeOz/rock/issues",
        "source": "https://github.com/romeOz/rock"
    },
    "authors": [
        {
            "name": "Ruslan",
            "email": "serggalka@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/romeOz/rock-base"
        },
        {
            "type": "vcs",
            "url": "https://github.com/romeOz/rock-file"
        },
        {
            "type": "vcs",
            "url": "https://github.com/romeOz/rock-execute"
        },
        {
            "type": "vcs",
            "url": "https://github.com/romeOz/cartographer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "romeoz/rock-base" : "*@dev",
        "romeoz/rock-helpers" : "*@dev",
        "romeoz/rock-di" : "*@dev",
        "romeoz/rock-sanitize" : "*@dev",
        "romeoz/rock-validate" : "*@dev",
        "romeoz/rock-events" : "*@dev",
        "romeoz/rock-request" : "*@dev",
        "romeoz/rock-url" : "*@dev",
        "romeoz/rock-date" : "*@dev",
        "romeoz/rock-i18n" : "*@dev",
        "romeoz/rock-file" : "*@dev",
        "romeoz/rock-cache" : "*@dev",
        "romeoz/rock-execute" : "*@dev",
        "filp/whoops": "*",
        "phpmailer/phpmailer"   : "*",
        "imagine/imagine": "*",
        "monolog/monolog": "*",
        "league/flysystem": "0.5.*",
        "cebe/markdown": "*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.4.*",
        "videlalvaro/php-amqplib": "2.2.*",
        "lusitanian/oauth": "*",
        "mibe/feedwriter": "*",
        "tackk/cartographer" :"*@dev"
    },
    "suggest": {
        "tackk/cartographer": "required by SitemapResponseFormatter",
        "mibe/feedwriter": "required by RssResponseFormatter",
        "lusitanian/oauth" : "required by rock-authclient extension",
        "videlalvaro/php-amqplib": "required by rock-mq extension"
    },
    "autoload": {
        "psr-4": {
            "rock\\": "framework/core/",
            "rock\\snippets\\": "framework/snippets/",
            "rock\\imagine\\": "framework/extensions/imagine/",
            "rock\\sphinx\\": "framework/extensions/sphinx/",
            "rock\\mongodb\\": "framework/extensions/mongodb/",
            "rock\\authclient\\" : "framework/extensions/authclient/",
            "rock\\mq\\" : "framework/extensions/mq/",
            "apps\\": "apps/"
        },
        "files": ["framework/extensions/phpmorphy/src/common.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}